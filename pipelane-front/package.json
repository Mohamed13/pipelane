{
  "name": "pipelane-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "node tools/inject-env.mjs",
    "start": "ng serve --port 4200",
    "prebuild": "node tools/inject-env.mjs",
    "build": "ng build",
    "pretest": "node tools/inject-env.mjs",
    "test": "jest --passWithNoTests",
    "pree2e": "node tools/inject-env.mjs",
    "e2e": "cypress run",
    "gen:swagger": "node tools/fetch-swagger.mjs",
    "gen:types": "openapi-typescript src/app/api/swagger.json -o src/app/api/openapi-types.ts",
    "gen:api": "npm run gen:swagger && npm run gen:types",
    "lint": "eslint ."
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "^7.8.0",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular/cli": "^17.3.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "cypress": "^13.13.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "ts-jest": "^29.2.5",
    "openapi-typescript": "^6.7.6",
    "typescript": "~5.4.0"
  }
}
