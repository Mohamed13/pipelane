{
  "name": "pipelane-marketing",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint \"src/**/*.{ts,tsx,astro}\"",
    "format": "prettier --cache --write \"**/*.{ts,tsx,astro,js,jsx,json,md,css}\"",
    "preview:ci": "astro preview --host 127.0.0.1 --port 4321",
    "test:a11y": "start-server-and-test preview:ci http://127.0.0.1:4321 \"pa11y-ci\"",
    "test:lighthouse": "start-server-and-test preview:ci http://127.0.0.1:4321 \"lighthouse http://127.0.0.1:4321 --only-categories=accessibility --quiet --chrome-flags=\\\"--headless\\\" --score-threshold=0.95\""
  },
  "dependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "@fontsource-variable/inter": "^5.0.6",
    "astro": "^5.15.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.33.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "lighthouse": "^10.4.0",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.4.5"
  }
}
